{
  "name": "elizabeth-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src tests --ext .js,.ts,.vue",
    "lint:fix": "eslint src tests --ext .js,.ts,.vue --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run",
    "e2e": "playwright test",
    "check": "npm run lint && npm run test && npm run build && npm run e2e"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "vue": "^3.4.30",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^25.0.0",
    "@playwright/test": "^1.47.0",
    "prettier": "^3.3.3",
    "sass": "^1.77.6",
    "vite": "^5.2.12",
    "vitest": "^2.0.5"
  }
}
